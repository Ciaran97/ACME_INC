@model IEnumerable<MVC_Test.ORDER>

@{
    ViewBag.Title = "CustOrders";
}

<h2>My Orders</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DATE_ORDERED)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.COURIER)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AspNetUser.Email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DATE_ORDERED)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.COURIER)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AspNetUser.Email)
        </td>
      
    </tr>
}

</table>
